{
  "name": "duplicant",
  "displayName": "Duplicant Visualizer",
  "description": "Visualizer for Duplicant data dumps",
  "repository": "https://github.com/qix-/duplicant",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "duplicant.showheatmap",
        "title": "Show Duplicant Heatmap"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "rm -rf dist && webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
	"vsix": "vsce package --no-dependencies -o dist/"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
